<?xml version="1.0" encoding="UTF-8"?>
<phpunit
	bootstrap="php/bootstrap.php"
	backupGlobals="false"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true"
>
	<testsuites>
		<testsuite name="unit">
			<directory suffix="Test.php">php</directory>
		</testsuite>
	</testsuites>
</phpunit>
